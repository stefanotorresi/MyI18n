{
    "name": "stefanotorresi/my-i18n",
    "description": "A Zend Framework 2 module for locale detection and management.",
    "type": "library",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "i18n",
        "locale"
    ],
    "license": "MIT",
    "homepage": "https://github.com/stefanotorresi/MyI18n",
    "authors": [
        {
            "name": "Stefano Torresi",
            "email": "webdev@stefanotorresi.it",
            "homepage": "http://stefanotorresi.it"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.1",
        "zendframework/zendframework": "~2.2",
        "gedmo/doctrine-extensions": "~2.3",
        "doctrine/doctrine-module": "~0.8",
        "doctrine/doctrine-orm-module": "~0.8",
        "stefanotorresi/my-base": "1.*"
    },
    "suggest": {
        "ext-intl" : "Intl extension is pretty much a requirement for this module"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-0.x": "0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "MyI18n": "src/"
        },
        "classmap": [
            "./"
        ]
    }
}
